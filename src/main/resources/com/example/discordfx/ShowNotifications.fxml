<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<?import java.net.URL?>

<AnchorPane fx:id="background" prefHeight="700.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.discordfx.ShowNotifications">
   <stylesheets>
      <URL value="@StyleSheet/ShowNotifications.css" />
   </stylesheets>
   <children>
      <Pane fx:id="pane" layoutX="79.0" layoutY="101.0" prefHeight="519.0" prefWidth="742.0">
         <children>
            <TextArea fx:id="textArea" editable="false" layoutX="50.0" layoutY="107.0" prefHeight="306.0" prefWidth="643.0" wrapText="true">
               <font>
                  <Font size="19.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="TEXT" />
               </cursor>
            </TextArea>
            <Button layoutX="246.0" layoutY="430.0" mnemonicParsing="false" onAction="#deleteNotifications" prefHeight="44.0" prefWidth="251.0" text="Delete Notification" textFill="#b91f1f">
               <font>
                  <Font size="15.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Button>
            <Button layoutX="612.0" layoutY="462.0" mnemonicParsing="false" onAction="#backToMenu" prefHeight="44.0" prefWidth="92.0" text="Back">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Button>
            <Label fx:id="iconContainer" layoutX="291.0" layoutY="-81.0">
               <graphic>
                  <ImageView fx:id="icon" fitHeight="152.0" fitWidth="152.0">
                     <image>
                        <Image url="@../../../../../../Icon/Notification.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Label>
         </children>
      </Pane>
   </children>
</AnchorPane>
